(pp
 (loop
  with ht = (make-hash-table :test #'equal)
  for (header-name header-value list-name) in old-gnus-splitters
  do (push (cons header-value list-name) (gethash header-name ht (list)))
  finally
  (loop 
   for k being the hash-keys of ht using (hash-values v)
   do (message "k: %S v: %S" k v))))


(flet ((mht () (make-hash-table :test #'equal)))
  (let ((L '((h1 v1 l1)
	     (h1 v2 l1)
	     (h1 v3 l2)
	     (h1 v4 l2)
	     (h2 v5 l1)
	     (h2 v6 l2)
	     (h3 v7 l2)))
	)
    (loop
     with ht = (make-hash-table :test #'equal)
     for (header-name header-value list-name) in L
     do
     (cond 
      (nil
       (push (cons header-value list-name) (gethash header-name ht (list))))
      (t
       (message "inside: hv: %s hn: %s ln: %s"
		header-value header-name list-name)
       (let ((lht (or (gethash list-name ht)
		      (setf (gethash list-name ht) (mht)))))
	 (push header-value
	       (gethash header-name
			lht))))
      (nil
       (push header-value
	     (gethash header-name
		      (gethash list-name ht (mht))
		      '()))))
     finally
     (loop 
      for k being the hash-keys of ht using (hash-values v)
      do (loop for j being the hash-keys of v using (hash-values w)
	       do (message "k: %S v: %S j: %S w: %S" k v j w)))
     )))

;; Munging with gmh/pmh
(let ((L
       (if nil
	   '((h1 v1 l1)
	     (h1 v2 l1)
	     (h1 v3 l2)
	     (h1 v4 l2)
	     (h2 v5 l1)
	     (h2 v6 l2)
	     (h3 v7 l2))
	 old-gnus-splitters)))
  (loop
   with ht = (make-hash-table :test #'equal)
   for (header-name header-value list-name) in L
   do (setf (gmh (list header-name header-value) ht) list-name)
   finally
   (progn
     (let ((buf (get-buffer-create " *Splitters*")))
       (with-current-buffer buf
	 (erase-buffer)
	 (loop 
	  for k being the hash-keys of ht using (hash-values v)
	  do (loop for j being the hash-keys of v using (hash-values w)
		   do (insert (format "k: %S j: %S w: %S\n" k j w)))))
       (display-buffer buf))
     (let ((buf (get-buffer-create " *MH-E splitters*")))
       (with-current-buffer buf
	 (erase-buffer)
	 (pp
	  (loop for k being the hash-keys of ht using (hash-values v)
	   collect (cons k (loop for l being the hash-keys of v using (hash-values w)
				 collect (cons l w))))
	  (current-buffer))
	 (display-buffer buf))))))

